{
  "name": "@dlghq/dialog-components",
  "version": "0.6.3",
  "description": "Dialog Web Components",
  "main": "src/index.js",
  "files": [
    "src"
  ],
  "keywords": [
    "react",
    "components",
    "dialog"
  ],
  "scripts": {
    "start": "styleguidist server",
    "create": "node scripts/create-component.js",
    "build": "npm-run-all --parallel build:*",
    "build:docs": "styleguidist build",
    "build:lib": "node scripts/export.js",
    "pretest": "npm run lint",
    "test": "exit 0",
    "lint": "npm-run-all --parallel lint:*",
    "lint:js": "eslint src/",
    "lint:css": "stylelint 'src/**/*.css'",
    "format": "npm-run-all --parallel format:*",
    "format:js": "eslint src/ --fix",
    "format:css": "find src -path '**/*.css' -exec stylefmt {} \\;",
    "prerelease": "npm run build",
    "release": "npm publish",
    "postrelease": "npm run gh-pages",
    "gh-pages": "buildbranch gh-pages styleguide"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dialogs/dialog-web-components.git"
  },
  "author": "Nikita Gusakov <gusnkt@gmail.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/dialogs/dialog-web-components/issues"
  },
  "homepage": "https://dialogs.github.io/dialog-web-components",
  "dependencies": {
    "classnames": "2.2.5",
    "intl": "^1.2.4",
    "lodash": "4.13.1",
    "react": "15.2.1",
    "react-dom": "15.2.1",
    "react-intl": "2.1.3",
    "react-modal": "1.4.0"
  },
  "devDependencies": {
    "@dlghq/babel-preset-dialog": "1.3.1",
    "@dlghq/eslint-config-dialog": "1.1.0",
    "@dlghq/postcss-dialog": "1.0.1",
    "@dlghq/stylelint-config-dialog": "0.4.0",
    "buildbranch": "1.0.0",
    "chalk": "1.1.3",
    "commander": "2.9.0",
    "ejs": "2.4.2",
    "eslint": "3.0.1",
    "eslint-plugin-react": "5.2.2",
    "inquirer": "1.1.2",
    "npm-run-all": "2.3.0",
    "postcss": "5.0.21",
    "postcss-loader": "0.9.1",
    "react-styleguidist": "3.0.0",
    "stylefmt": "4.1.1",
    "stylelint": "6.9.0",
    "yaml-loader": "0.2.0"
  }
}
