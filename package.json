    "chalk": "1.1.3",
    "ejs": "2.5.6",
    "eslint": "3.19.0",
    "eslint-plugin-react": "6.10.3",
    "file-loader": "0.11.1",
    "flow-bin": "0.46.0",
    "flow-coverage-report": "0.3.0",
