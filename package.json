{
  "name": "@dlghq/dialog-components",
  "version": "0.15.0",
  "description": "Dialog Web Components",
  "main": "src/index.js",
  "files": [
    "src"
  ],
  "keywords": [
    "react",
    "components",
    "dialog"
  ],
  "scripts": {
    "start": "styleguidist server",
    "create": "node scripts/create-component.js",
    "build": "npm-run-all --parallel build:*",
    "build:docs": "styleguidist build",
    "build:lib": "node scripts/export.js",
    "build:l10n": "node scripts/compile-translations.js",
    "pretest": "npm run lint",
    "test": "exit 0",
    "lint": "npm-run-all --parallel lint:js",
    "lint:js": "eslint src/",
    "lint:css": "stylelint 'src/**/*.css'",
    "format": "npm-run-all --parallel format:*",
    "format:js": "eslint src/ --fix",
    "format:css": "find src -path '**/*.css' -exec stylefmt {} \\;",
    "preversion": "npm test",
    "prerelease": "npm run build",
    "release": "npm publish",
    "postrelease": "npm run gh-pages",
    "gh-pages": "buildbranch gh-pages styleguide"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dialogs/dialog-web-components.git"
  },
  "author": "Nikita Gusakov <gusnkt@gmail.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/dialogs/dialog-web-components/issues"
  },
  "homepage": "https://dialogs.github.io/dialog-web-components",
  "dependencies": {
    "@dlghq/react-l10n": "0.1.2",
    "classnames": "2.2.5",
    "lodash": "4.15.0",
    "react": "15.3.1",
    "react-dom": "15.3.1",
    "react-modal": "1.4.0"
  },
  "devDependencies": {
    "@dlghq/babel-preset-dialog": "1.4.1",
    "@dlghq/eslint-config-dialog": "1.5.0",
    "@dlghq/postcss-dialog": "1.0.2",
    "@dlghq/stylelint-config-dialog": "0.6.1",
    "buildbranch": "1.0.0",
    "chalk": "1.1.3",
    "commander": "2.9.0",
    "ejs": "2.5.1",
    "eslint": "3.4.0",
    "eslint-plugin-react": "6.2.0",
    "glob": "7.0.6",
    "inquirer": "1.1.3",
    "npm-run-all": "3.1.0",
    "postcss-loader": "0.11.1",
    "react-styleguidist": "3.1.1",
    "stylefmt": "4.2.3",
    "stylelint": "7.2.0",
    "yml-loader": "1.1.0"
  }
}
