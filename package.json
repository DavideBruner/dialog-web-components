    "@dlghq/emoji": "2.0.1",
    "@dlghq/markdown": "1.1.0",
    "@dlghq/react-l10n": "0.9.1",
    "classnames": "2.2.5",
    "clipboard": "1.7.1",
    "croppie": "2.6.1",
